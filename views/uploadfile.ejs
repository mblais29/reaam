<!-- enctype="multipart/form-data" -->
<form id="uploadForm"
      enctype="multipart/form-data"
      action="/file/upload"
      method="post">
      	<input type="text" name="id" value="<%= req.session.User.id %>" readonly />
    	<input type="file" name="uploadFile" />
    	<input type="submit" value="submit"/>
    	<input type="hidden" name="_csrf" value="<%= _csrf %>" />
</form>